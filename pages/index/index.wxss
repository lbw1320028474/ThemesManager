.container {
  display: flex;
  flex-direction: column;
}

.headView1 {
  width: 100rpx;
  height: 100rpx;
  background-color: #f00;
}

.tabview {
  height: 100rpx;
  width: 750rpx;
  background-color: #fff;
  position: fixed;
  bottom: 0;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.tab_item {
  display: flex;
  align-items: center;
  justify-content: center;
  flex: 1;
}

.scrollview_x {
  white-space: nowrap;
  height: 350rpx;
}

